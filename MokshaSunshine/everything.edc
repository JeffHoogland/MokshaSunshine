         }
      }
   }

   group {
      name: "e/modules/everything/tab_item/back";
      min: 50 20;
      parts {
         part {
            name: "clip";
	    type: RECT;
	    mouse_events: 0;
            description {
               state: "default" 0.0;
            rel1.relative: 0.00 0.0;
            rel2.relative: 1.0 0.9;
            }
         }
         part {
            name: "label2";
            type: TEXT;
