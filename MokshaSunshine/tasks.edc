      part { name: "icon_area";
         description { state: "default" 0.0;
            align: 0.5 0.5;
            rel1.offset: 8 8;
            rel2.relative: 1.0 1.0;
            rel2.offset: -9 -9;
         }  
      }
   }
}

   
