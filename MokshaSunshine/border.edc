		state: "default" 0.1;				\
		inherit: "default" 0.0;				\
		min: MIN; 					\
		max: MAX; 					\
		rel1 { 						\
			relative: RELF1; 			\
			offset: OFF1; 				\
			to_y: "top-center";			\
		}						\
								\
		rel2 { 						\
			relative: RELF2; 			\
			offset: OFFF2; 				\
			to_y: "top-center";			\
		} 						\
		visible: 0;					\
	}							\
}

