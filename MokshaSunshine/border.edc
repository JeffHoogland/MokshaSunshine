#define BORDER(NAME, MIN, MAX, REL1, OFF1, REL2, OFF2, IMG1, RELF1, RELF2, OFFF2) 	\
part { 								\
	name: NAME; 						\
	type: IMAGE;						\
	mouse_events: 0; 					\
								\
	description { 						\
		state: "default" 0.0; 				\
		min: MIN; 					\
		max: MAX; 					\
		fixed: 1 1;					\
								\
		rel1 { 						\
			relative: REL1; 			\
			offset: OFF1; 				\
		}						\
								\
		rel2 { 						\
			relative: REL2; 			\
			offset: OFF2; 				\
		} 						\
								\
