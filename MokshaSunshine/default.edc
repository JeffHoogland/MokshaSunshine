				description { state: "default" 0.0;
					rel1 {
						relative: 0 1;
						offset: 20 -15;
						to: "e.swallow.background";
					}
					rel2 {
						relative: 1 1;
						offset: -20 0;
						to: "e.swallow.background";
					}
					color: 186 202 218 150;
