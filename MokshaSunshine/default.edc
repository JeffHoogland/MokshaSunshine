	group {												\
		name: "e/modules/engage/bg_"POS;				\
		images.image: IMG COMP;								\
		parts {											\
			part {										\
				name: "bg";								\
				type: IMAGE;							\
				mouse_events: 0;						\
				description { state: "default" 0.0;		\
					rel1 {								\
						relative: 0.0 0.0;				\
						offset: OFF1;					\
					}									\
					rel2 {								\
						relative: 1.0 1.0;				\
						offset: OFF2;					\
					}									\
					image {								\
						normal: IMG;					\
						border: BORDER;					\
