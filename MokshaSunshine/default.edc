			}
			part { name: "e.edge.bottom_left";
				type: RECT;
				description { state: "default" 0.0;
					rel1 {
						relative: 0 1;
						offset: 0 -20;
						to: "e.swallow.background";
					}
					rel2 {
						relative: 0 1;
						offset: 20 0;
						to: "e.swallow.background";
					}
